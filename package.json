{
  "name": "react-chatbot",
  "version": "1.0.0",
  "description": "React chatbot for webpage",
  "main": "index.js",
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node index.js",
    "backend": "nodemon index.js",
    "frontend": "npm run start --prefix client-chatbot",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client-chatbot && npm run build --prefix client-chatbot"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dialogflow": "^0.10.1",
    "dialogflow-fulfillment": "^0.3.0-beta.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.6.2",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.1"
  }
}
